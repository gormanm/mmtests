#!/bin/bash
SHELLPACK_ROOT=$(dirname $(realpath -s ${BASH_SOURCE[0]}))
SHELLPACK_BIN=$SHELLPACK_ROOT/../../parsers

LOGDIR=${1:-`pwd`}
cd $LOGDIR || exit 1

for WORKLOAD in `\ls *.log | sed -e 's/\.log$//'`; do
	VALUE=`grep "mean of 95%" ${WORKLOAD}.log | awk '{print $NF}'`
	echo -e "mean95\t$WORKLOAD\t_\t1\t$VALUE\t_"
done
