### tbench4 is the network portion of the dbench4 benchmark.

export MMTESTS="tbench4"

# List of monitors
export RUN_MONITOR=yes
export MONITORS_ALWAYS=
export MONITORS_GZIP="proc-vmstat mpstat turbostat"
export MONITORS_WITH_LATENCY="vmstat"
export MONITOR_UPDATE_FREQUENCY=1

# TBench
export TBENCH_DURATION=120
export TBENCH_MIN_CLIENTS=1
export TBENCH_MAX_CLIENTS=$((NUMCPUS*2))

export MMTESTS_THREAD_CUTOFF=
