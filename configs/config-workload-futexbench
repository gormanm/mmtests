export MMTESTS="futexbench-hash futexbench-requeue futexbench-requeue-pi futexbench-wake futexbench-wake-parallel futexbench-lock-pi"


# List of monitors
export RUN_MONITOR=yes
export MONITORS_ALWAYS=
export MONITORS_GZIP="proc-vmstat top"
export MONITORS_WITH_LATENCY="vmstat"
export MONITOR_UPDATE_FREQUENCY=10

# futexbench
export FUTEXBENCH_MIN_THREADS=2
export FUTEXBENCH_MAX_THREADS=$((NUMCPUS*64))
export FUTEXBENCH_SHARED=no
